meta {
  name: Create Session
  type: http
  seq: 1
}

post {
  url: http://{{base-url}}/api/session
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "name": "Jordan",
    "title": "Movie night!",
    "config": {
      "anonymity": false,
      "voting_mode": "ranked_choice",
      "min_choices": 1,
      "max_choices": 3,
      "grace_period_seconds": 60,
      "allow_empty_voters": false
    }
  }
}

settings {
  encodeUrl: false
  timeout: 0
}
